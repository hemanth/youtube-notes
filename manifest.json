{
  "manifest_version": 2,
  "name": "yt-notes",
  "version" : "1.0.3",
  "icons" : {
    "16" : "notepad.gif",
    "128" : "notes.png"
  },
  "permissions" : ["tabs","http://*.youtube.com/*"],
   "content_scripts": [
    {
        "matches": ["http://*.youtube.com/*"],
        "js": ["notes.js"]
    }
  ]

}
